                          ] ;
        owl:disjointWith  :PetitPoisTopping , :MushroomTopping , :RocketTopping , :ArtichokeTopping , :OliveTopping , :SpinachTopping , :PepperTopping , :LeekTopping , :CaperTopping , :GarlicTopping , :TomatoTopping , :AsparagusTopping .

:FishTopping  a           owl:Class ;
