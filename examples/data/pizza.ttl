:AmericanHot  a           owl:Class ;
        rdfs:label        "AmericanaPicante"@pt ;
        rdfs:subClassOf   :NamedPizza ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      :hasTopping ;
                            owl:someValuesFrom  :TomatoTopping
                          ] ;
        rdfs:subClassOf   [ a               owl:Restriction ;
                            owl:hasValue    :America ;
                            owl:onProperty  :hasCountryOfOrigin
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      :hasTopping ;
                            owl:someValuesFrom  :PeperoniSausageTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      :hasTopping ;
                            owl:someValuesFrom  :MozzarellaTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      :hasTopping ;
                            owl:someValuesFrom  :HotGreenPepperTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      :hasTopping ;
                            owl:someValuesFrom  :JalapenoPepperTopping
                          ] ;
        rdfs:subClassOf   [ a                  owl:Restriction ;
                            owl:allValuesFrom  [ a            owl:Class ;
                                                 owl:unionOf  ( :MozzarellaTopping :PeperoniSausageTopping :JalapenoPepperTopping :TomatoTopping :HotGreenPepperTopping )
                                               ] ;
                            owl:onProperty     :hasTopping
                          ] ;
        owl:disjointWith  :Giardiniera , :FruttiDiMare , :UnclosedPizza , :Fiorentina , :Caprina , :FourSeasons , :Parmense , :Rosa , :Cajun , :American , :PrinceCarlo , :PolloAdAstra , :Mushroom , :SloppyGiuseppe , :Siciliana , :Margherita , :Napoletana , :Soho , :Veneziana , :LaReine , :QuattroFormaggi , :Capricciosa .

