<http://www.co-ode.org/ontologies/pizza/pizza.owl>
        a                owl:Ontology ;
        rdfs:comment     "An example ontology that contains all constructs required for the various versions of the Pizza Tutorial run by Manchester University (see http://www.co-ode.org/resources/tutorials/)"@en ;
        owl:versionInfo  "v.1.4. Added Food class (used in domain/range of hasIngredient), Added several hasCountryOfOrigin restrictions on pizzas, Made hasTopping invers functional"@en , "version 1.5"^^xsd:string , "v.1.5. Removed protege.owl import and references. Made ontology URI date-independent"@en .

:SpicyPizza  a               owl:Class ;
        rdfs:comment         "Any pizza that has a spicy topping is a SpicyPizza"@en ;
        rdfs:label           "PizzaTemperada"@pt ;
        owl:equivalentClass  [ a                   owl:Class ;
                               owl:intersectionOf  ( :Pizza _:b4 )
                             ] .

_:b5    a            owl:Class ;
        owl:unionOf  ( :SauceTopping :CheeseTopping :VegetableTopping :FruitTopping :NutTopping :HerbSpiceTopping ) .

:DeepPanBase  a           owl:Class ;
        rdfs:label        "BaseEspessa"@pt ;
        rdfs:subClassOf   :PizzaBase ;
        owl:disjointWith  :ThinAndCrispyBase .

:Food   a                owl:Class ;
        rdfs:subClassOf  :DomainConcept .

:AnchoviesTopping  a      owl:Class ;
        rdfs:label        "CoberturaDeAnchovies"@pt ;
        rdfs:subClassOf   :FishTopping ;
        owl:disjointWith  :MixedSeafoodTopping , :PrawnsTopping .

:CaperTopping  a          owl:Class ;
