                          ] ;
        owl:disjointWith  :MushroomTopping , :ArtichokeTopping , :LeekTopping , :SpinachTopping , :GarlicTopping , :PetitPoisTopping , :TomatoTopping , :OliveTopping , :AsparagusTopping , :OnionTopping , :RocketTopping , :PepperTopping .

:OnionTopping  a          owl:Class ;
